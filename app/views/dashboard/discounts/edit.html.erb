<%= form_for [:dashboard, @discount], url: dashboard_discount_path, method: :put do |f| %>

<p>
  <%= f.label :description %>
  <%= f.text_field :description %>
</p>

<p>
  <%= f.label :minimum_quantity %>
  <%= f.number_field :minimum_quantity %>
</p>

<p>
  <%= f.label :discount_amount %>
  <%= f.number_field :discount_amount %>
</p>

<p>
  <%= f.submit "Submit" %>
</p>
<% end %>
